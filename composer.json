{
    "name": "laravel-notification-channels/trello",
    "description": "Trello Notifications driver",
    "homepage": "https://github.com/laravel-notification-channels/trello",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com",
            "homepage": "http://www.marcelpociot.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "guzzlehttp/guzzle": "~6.0 || ~7.0",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "8.*",
        "orchestra/testbench": "~4.2 || ~5.0 || ~6.0",
        "orchestra/database": "~4.0 || ~5.0 || ~6.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Trello\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Trello\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
